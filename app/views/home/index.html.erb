		<!-- open/close -->
		<header>
			<%=render 'home/partials/banner_section'%>
		</header>
		<section class="video" id="about">
			<%=render 'home/partials/pm'%>
		</section>
		<section class="features-intro">
			<%=render 'home/partials/corporate_trainers'%>
		</section>
		<section class="features-list" id="features">
			<%=render 'home/partials/courses'%>
		</section>
		
		<section class="download" id="download">
			<%=render 'home/partials/testimonials'%>
		</section>
		<footer>
			<div class="container">
				<div class="row">
					<div class="col-md-5">
						<h1 class="footer-logo">
						<img src="<%=asset_path 'logo-dark.png'%>" alt="Footer Logo Blue">
						</h1>
						<p id="ftext"><a href="<%=LogoText.last.url%>" ><%=LogoText.last.logo_bottom_text%></a></p>
					</div>
					<div class="col-md-7">
						<ul class="footer-nav">
							<%MenuItem.all.each do |m|%>
								<li style="color: #3f6184;"><a class="fitem" href="<%=m.url%>" ><%=m.title.upcase%></a></li>
							<%end%>
						</ul>
					</div>
				</div>
			</div>
		</footer>
		<div class="overlay overlay-boxify">
			<%=render 'home/partials/overlay_menu'%>
		</div>
		
	<%=javascript_tag do%>
		


		$('#trigger-overlay').on('click',function(){
		 
		if($(this).attr('data-click-state') == 1) {
		$(this).attr('data-click-state', 0)
		/*Change this to your own property / function - Click State 1*/
		$('#trigger-overlay').addClass('active');
		$('div.overlay-boxify').addClass('open');
		/*Click State 1 finish*/
		} else {
		$(this).attr('data-click-state', 1)
		/*Change this to your own property / function - Click State 2*/
		$('#trigger-overlay').removeClass('active');
		$('div.overlay-boxify').removeClass('open');
		/*Click State 2 finish*/
		}
		 
		});

	<%end%>





